<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.progzc.ke.mapper.InfoMapper">
    <select id="queryIdListByMenuId" resultType="java.lang.Integer">
        select id from info where menu_id = #{id}
    </select>
    <select id="queryIds" resultType="com.progzc.ke.entity.Info">
        select id,course_name from info
    </select>
</mapper>
